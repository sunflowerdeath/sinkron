{
    "name": "@sinkron/client",
    "version": "0.0.1",
    "description": "",
    "scripts": {
        "lint": "oxlint -c ./oxlintrc.json src",
        "build": "tsc && NODE_ENV=production rspack --config rspack.config.js",
        "test:sinkron": "SINKRON_CONFIG=$(cat config.test.json) docker compose -f ./test-compose.yml up -d --build --renew-anon-volumes --force-recreate",
        "test:build": "rspack --config rspack.config.test.js",
        "test:run": "mocha ./build/test.js",
        "test": "pnpm test:sinkron && pnpm test:build && pnpm test:run",
        "benchmark:sinkron:perf": "RUST_LOG=sinkron=warn SINKRON_CONFIG=$(cat config.benchmark.json) sudo -E flamegraph -- ../../rust/target/benchmark/sinkron",
        "benchmark:sinkron": "SINKRON_CONFIG=$(cat config.benchmark.json) ../../rust/target/release/sinkron",
        "benchmark:client": "rspack --config rspack.config.benchmark.js && node ./build/benchmark.js"
    },
    "keywords": [],
    "author": "sunflowerdeath",
    "license": "Unlicense",
    "devDependencies": {
        "@rspack/cli": "^1.0.13",
        "@rspack/core": "^1.0.13",
        "@types/mocha": "^10.0.9",
        "@types/uuid": "^9.0.8",
        "@types/ws": "^8.5.13",
        "mocha": "^10.7.3",
        "pino-pretty": "^10.3.1",
        "ws": "^8.18.0"
    },
    "dependencies": {
        "@types/lodash": "^4.17.13",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^22.8.6",
        "date-fns": "^3.6.0",
        "dotenv": "^16.4.5",
        "js-base64": "^3.7.7",
        "lodash": "^4.17.21",
        "lodash-es": "^4.17.21",
        "loro-crdt": "^1.0.8",
        "mobx": "^6.13.3",
        "nanoevents": "^7.0.1",
        "nanoid": "^4.0.2",
        "pino": "^9.5.0",
        "sinkron-protocol": "workspace:*",
        "uuid": "^9.0.1"
    }
}
